            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineStatureTop"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.28"/>

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineStatureBottom"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.35"/>



    <gcatech.net.documentcapturepicture.customviews.MappingRectangleCustomView
            android:layout_height="0dp"
            android:layout_width="0dp"
            app:propName="stature"
            app:codeBarMapping="false"
            android:background="@color/backgroundCrp"
            app:layout_constraintLeft_toRightOf="@id/guideLineVerticalLineStatureLeft"
            app:layout_constraintRight_toLeftOf="@id/guideLineVerticalLineStatureRight"
            app:layout_constraintBottom_toTopOf="@+id/guideLineVerticalLineStatureBottom"
            app:layout_constraintTop_toBottomOf="@id/guideLineVerticalLineStatureTop"/>


    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineRhLeft"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.53"/>

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineRhRight"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.66"/>

    <gcatech.net.documentcapturepicture.customviews.MappingRectangleCustomView
            android:layout_height="0dp"
            android:layout_width="0dp"
            app:propName="rh"
            app:codeBarMapping="false"
            android:background="@color/backgroundCrp"
            app:layout_constraintLeft_toRightOf="@id/guideLineVerticalLineRhLeft"
            app:layout_constraintRight_toLeftOf="@id/guideLineVerticalLineRhRight"
            app:layout_constraintBottom_toTopOf="@+id/guideLineVerticalLineStatureBottom"
            app:layout_constraintTop_toBottomOf="@id/guideLineVerticalLineStatureTop"/>


