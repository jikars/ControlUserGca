            app:layout_constraintGuide_percent="0.89"/>

    <gcatech.net.documentcapturepicture.customviews.MappingRectangleCustomView
            android:layout_height="0dp"
            android:layout_width="0dp"
            app:propName="codeBar"
            app:codeBarMapping="true"
            android:background="@color/backgroundCrp"
            app:layout_constraintLeft_toRightOf="@id/guideLineVerticalLineCodeBarPlaceLeft"
