            app:propName="birthPlace"
            app:codeBarMapping="false"
            android:background="@color/backgroundCrp"
            app:layout_constraintLeft_toRightOf="@id/guideLineVerticalLineBirthPlaceLeft"
            app:layout_constraintRight_toLeftOf="@id/guideLineVerticalLineBirthPlaceRight"
            app:layout_constraintBottom_toTopOf="@+id/guideLineVerticalLineBirthPlaceBottom"
            app:layout_constraintTop_toBottomOf="@id/guideLineVerticalLineBirthPlaceTop"/>


    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineBirthDateLeft"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.65"/>

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineBirthDateRight"
