            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineExpeditionPlaceBottom"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.47"/>

    <gcatech.net.documentcapturepicture.customviews.MappingRectangleCustomView
            android:layout_height="0dp"
            android:layout_width="0dp"
            app:propName="expeditionPlaceAndDate"
            app:codeBarMapping="false"
            android:background="@color/backgroundCrp"
            app:layout_constraintLeft_toRightOf="@id/guideLineVerticalLineExpeditionPlaceLeft"
            app:layout_constraintRight_toLeftOf="@id/guideLineVerticalLineExpeditionPlaceRight"
            app:layout_constraintBottom_toTopOf="@+id/guideLineVerticalLineExpeditionPlaceBottom"
