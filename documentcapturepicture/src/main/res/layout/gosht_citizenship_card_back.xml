<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineFingerPrintLeft"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.03"/>

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineFingerPrintRight"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.33"/>

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineFingerPrintNumberTop"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.065"/>

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineFingerPrintBottom"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.51"/>

    <gcatech.net.documentcapturepicture.customviews.MappingRectangleCustomView
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:propName="fingerPrint"
        app:fingerPrint="true"
        app:codeBarMapping="false"
        android:background="@color/backgroundCrp"
        app:layout_constraintLeft_toRightOf="@id/guideLineVerticalLineFingerPrintLeft"
        app:layout_constraintRight_toLeftOf="@id/guideLineVerticalLineFingerPrintRight"
        app:layout_constraintBottom_toTopOf="@+id/guideLineVerticalLineFingerPrintBottom"
        app:layout_constraintTop_toBottomOf="@id/guideLineVerticalLineFingerPrintNumberTop"/>

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineBirthPlaceLeft"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.37"/>

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineBirthPlaceRight"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.625"/>

    <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideLineVerticalLineBirthPlaceTop"
            android:orientation="horizontal"
